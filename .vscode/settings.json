{
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": "explicit"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "prettier.proseWrap": "always",
  "cSpell.words": [
    "abandoned",
    "fortrabbit",
    "hyperscaler",
    "serverful"
  ],
}